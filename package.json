{
  "name": "crystalline-giant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn clint && next dev",
    "compile": "tsc -p tsconfig.app.json && tsc -p tsconfig.test.json",
    "clint": "yarn compile && yarn lint",
    "build": "next build",
    "start": "next start",
    "test": "yarn clint && yarn jest --watch",
    "test:ci": "yarn clint && yarn jest",
    "lint": "eslint --fix --ext .js,.ts,.tsx ."
  },
  "dependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.6.1",
    "next": "9.5.5",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  }
}
